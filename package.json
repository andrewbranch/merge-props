{
  "name": "merge-props",
  "version": "1.0.0",
  "description": "Merges className, style, and DOM event handlers for React elements",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewbranch/merge-props.git"
  },
  "keywords": [
    "React",
    "merge",
    "props"
  ],
  "author": "Andrew Branch <andrew@wheream.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewbranch/merge-props/issues"
  },
  "homepage": "https://github.com/andrewbranch/merge-props#readme",
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/react": "^16.4.14",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.0",
    "typescript": "^3.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/?(*.)(spec|test).ts?(x)"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/test/",
      "/node_modules/"
    ]
  }
}
